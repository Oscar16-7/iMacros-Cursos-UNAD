SET !EXTRACT_TEST_POPUP NO
'SET !TIMEOUT_STEP 0
'SET !ERRORIGNORE YES

TAB T=1
TAG POS={{!LOOP}} TYPE=A ATTR=TXT:*gu*a<SP>de<SP>actividades* EXTRACT=HREF
SET !VAR1 {{!EXTRACT}}
SET !EXTRACT NULL

TAB OPEN NEW
TAB T=2
URL GOTO={{!VAR1}}
WAIT SECONDS=0.5
'TAG XPATH="//div[@id='settingsnav']/ul[1]/li[1]/ul[1]/li[1]/p/a" EXTRACT=HREF
TAG POS=1 TYPE=A ATTR=TXT:*gu*a<SP>de<SP>actividades*.pdf
WAIT SECONDS=1
