SET !EXTRACT_TEST_POPUP NO
SET !TIMEOUT_STEP 0
SET !ERRORIGNORE YES

TAB T=1
TAG POS=1 TYPE=A ATTR=TXT:*<SP>general<SP>* EXTRACT=HREF 
SET !VAR1 {{!EXTRACT}}
SET !EXTRACT NULL

TAB OPEN NEW
TAB T=2
URL GOTO={{!VAR1}}
TAG POS=1 TYPE=A ATTR=TXT:Editar<SP>ajustes<SP> EXTRACT=HREF 
SET !VAR1 {{!EXTRACT}}
URL GOTO={{!VAR1}}
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_maxbytes CONTENT=%5242880
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_maxattachments CONTENT=%9
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_displaywordcount CONTENT=%0
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_forcesubscribe CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_trackingtype CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_lockdiscussionafter CONTENT=%0
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_blockperiod CONTENT=%0
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_assessed CONTENT=%0
TAG POS=1 TYPE=FIELDSET FORM=ID:mform1 ATTR=ID:id_modstandardelshdr
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_visible CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_groupmode CONTENT=%0
TAG POS=1 TYPE=IMG ATTR=ALT:Borrar CONTENT={ENTER}
WAIT SECONDS= 0.5
TAG POS=1 TYPE=BUTTON:BUTTON ATTR=* CONTENT={ENTER}
TAG POS=1 TYPE=BUTTON:BUTTON ATTR=ID:availability_addrestriction_completion CONTENT={ENTER}
WAIT SECONDS= 0.5
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=NAME:cm CONTENT=$Normas<SP>y<SP>condiciones<SP>*
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=NAME:e CONTENT=%1
TAG POS=1 TYPE=INPUT:SUBMIT ATTR=ID:id_unlockcompletion CONTENT={ENTER}
WAIT SECONDS= 0.5
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_completion CONTENT=%0
TAG POS=1 TYPE=INPUT:SUBMIT ATTR=ID:id_submitbutton CONTENT={ENTER}
WAIT SECONDS= 1
TAB CLOSE
